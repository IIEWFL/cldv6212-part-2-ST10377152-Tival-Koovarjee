@model IEnumerable<ABC_Retail.Models.QueueLogViewModel>

    <h2>Log Messages</h2>

    <table class=" table">
        <thead>
            <tr>
                <th>Messge ID</th>
                <th>Insertion Time</th>
                <th>Message</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var log in Model)
            {
                <tr>
                    <td>@log.MessageId</td>
                    <td>@log.InsertionTime</td>
                    <td>@log.MessageText</td>
                </tr>
            
            }
        </tbody>
    </table>

    <form asp-action="ExportLog">
        <button type="submit" class="btn btn-primary">Export log Report</button>
    </form>
